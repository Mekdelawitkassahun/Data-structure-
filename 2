class Stack:
    def __init__(self):
        self.stack = []

    def push(self, item):
        self.stack.append(item)

    def pop(self):
        if not self.is_empty():
            return self.stack.pop()
        else:
            print("Stack is empty.")
            return None

    def is_empty(self):
        if self.stack==0:
            return True
        else:
            return False
        
    def size(self):
        return len(self.stack)
stack = Stack()
stack.push(10)
stack.push(20)
stack.push(30)

print(stack.size())

popped_item = stack.pop()
if popped_item is not None:
    print(popped_item)

print(stack.is_empty())